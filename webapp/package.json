{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc --p tsconfig.prod.json && vite build",
    "serve": "vite preview",
    "serve:e2e": "vite preview --port 3000",
    "generate:rtkapi": "npx @rtk-query/codegen-openapi openapi-config.ts",
    "test:unit": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .ts,.tsx",
    "eslint:check": "eslint src --ext .ts,.tsx,.js,.jsx",
    "eslint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx src",
    "tsc": "tsc",
    "prettier:check": "prettier 'src/**/*.(ts|tsx)' --check",
    "prettier:fix": "prettier 'src/**/*.(ts|tsx)' --write",
    "prettier": "prettier 'src/**/*.(ts|tsx)'",
    "storybook": "storybook dev -p 6006 -s ../public",
    "build-storybook": "storybook build -s public",
    "lint-staged": "lint-staged",
    "cypress:open": "cypress open --e2e -b chrome",
    "cypress:run": "cypress run",
    "cypress:open:component": "cypress open --component -b chrome",
    "cypress:run:component": "cypress run --component"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.9.8",
    "@mui/icons-material": "^5.10.16",
    "@mui/lab": "^5.0.0-alpha.118",
    "@mui/material": "^5.10.17",
    "@mui/styled-engine-sc": "^5.7.0",
    "@mui/x-data-grid": "^5.11.0",
    "@reduxjs/toolkit": "^1.8.1",
    "@uiw/react-md-editor": "^3.18.1",
    "axios": "^0.27.2",
    "cross-fetch": "^3.1.5",
    "dagre": "^0.8.5",
    "date-fns": "^2.28.0",
    "gzip-js": "^0.3.2",
    "html-to-image": "^1.10.8",
    "material-ui-popup-state": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-full-screen": "^1.1.1",
    "react-highlight-within-textarea": "^3.1.1",
    "react-hook-form": "^7.35.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-vega": "^7.6.0",
    "reactflow": "^11.8.3",
    "rehype-sanitize": "^5.0.1",
    "seqviz": "^3.7.6",
    "styled-components": "^5.3.5",
    "web-vitals": "^2.1.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@4tw/cypress-drag-drop": "^2.2.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@storybook/addon-actions": "^7.1.0",
    "@storybook/addon-essentials": "^7.1.0",
    "@storybook/addon-interactions": "^7.1.0",
    "@storybook/addon-links": "^7.1.0",
    "@storybook/addon-mdx-gfm": "^7.1.0",
    "@storybook/node-logger": "^7.1.0",
    "@storybook/react": "^7.1.0",
    "@storybook/react-vite": "^7.1.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/dagre": "^0.7.47",
    "@types/jest": "^29.2.5",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "@vitejs/plugin-react": "^2.2.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cypress": "13.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-plugin-tab": "^1.0.5",
    "cypress-vite": "^1.4.2",
    "emitter": "^0.0.5",
    "enforce-branch-name": "^1.1.2",
    "eslint": "^8.15.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.0.3",
    "msw": "^0.48.3",
    "msw-storybook-addon": "^1.8.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^7.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vega": "^5.22.1",
    "vega-lite": "^5.5.0",
    "vite": "^3.2.2",
    "vite-plugin-svgr": "^2.2.2",
    "vite-tsconfig-paths": "^3.5.2",
    "yaml": "^2.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
